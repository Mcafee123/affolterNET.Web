{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "Microsoft.AspNetCore": "Information",
      "affolterNET": "Debug",
      "Yarp": "Debug"
    }
  },
  "affolterNET": {
    "Web": {
      "BffOptions": {
        "FrontendUrl": "http://localhost:5173",
        "EnvironmentDisplayName": "DEV"
      },
      "SecurityHeaders": {
        "Enabled": false
      }
    },
    "ReverseProxy": {
      "Routes": {
        "api-route": {
          "ClusterId": "api-cluster",
          "Match": {
            "Path": "/api/{**catch-all}"
          }
        },
        "vite-hmr-websocket": {
          "ClusterId": "frontend-cluster",
          "Match": {
            "Path": "/",
            "QueryParameters": [
              {
                "Name": "token",
                "Mode": "Exists"
              }
            ]
          }
        },
        "vite-assets": {
          "ClusterId": "frontend-cluster",
          "Match": {
            "Path": "/assets/{**catch-all}"
          }
        },
        "vite-client": {
          "ClusterId": "frontend-cluster",
          "Match": {
            "Path": "/@vite/{**catch-all}"
          }
        },
        "vite-fs": {
          "ClusterId": "frontend-cluster",
          "Match": {
            "Path": "/@fs/{**catch-all}"
          }
        },
        "vite-id": {
          "ClusterId": "frontend-cluster",
          "Match": {
            "Path": "/@id/{**catch-all}"
          }
        },
        "vite-src": {
          "ClusterId": "frontend-cluster",
          "Match": {
            "Path": "/src/{**catch-all}"
          }
        },
        "vite-node-modules": {
          "ClusterId": "frontend-cluster",
          "Match": {
            "Path": "/node_modules/{**catch-all}"
          }
        },
        "vite-public": {
          "ClusterId": "frontend-cluster",
          "Match": {
            "Path": "/public/{**catch-all}"
          }
        },
        "vite-favicon": {
          "ClusterId": "frontend-cluster",
          "Match": {
            "Path": "/favicon.ico"
          }
        }
      },
      "Clusters": {
        "api-cluster": {
          "Destinations": {
            "api": {
              "Address": "https://localhost:5003"
            }
          }
        },
        "frontend-cluster": {
          "Destinations": {
            "frontend": {
              "Address": "http://localhost:5173"
            }
          },
          "HttpRequest": {
            "ActivityTimeout": "00:02:00",
            "AllowResponseBuffering": false
          }
        }
      }
    }
  }
}
